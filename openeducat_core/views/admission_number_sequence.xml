<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequences for op.student -->
        <record id="seq_op_student" model="ir.sequence">
            <field name="name">Admission Number</field>
            <field name="code">op.student</field>
            <field name="prefix">MA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="seq_op_student_mmc" model="ir.sequence">
            <field name="name">Memon Medical Code</field>
            <field name="code">op_student_mmc</field>
            <field name="prefix">MMC</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>
